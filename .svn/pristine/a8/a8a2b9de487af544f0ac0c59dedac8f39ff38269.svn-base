<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SpinnerLoader">
        <attr name="point_color" format="color"/>
        <attr name="isdynamic" format="boolean"/>
    </declare-styleable>


    <!--圆形头像属性-->
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>
    <declare-styleable  name="DropView" >
        <attr name="dropCount" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TasksCompletedView">
        <attr name="radius" format="dimension"/>
        <attr name="strokeWidth" format="dimension"/>
        <attr name="circleColor" format="color"/>
        <attr name="ringColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="BubbleCudgelView">
        <attr name="levelsum" format="integer"/>
        <attr name="levelcurr" format="integer"/>
        <attr name="textshow" format="string"/>
        <attr name="neutral" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CatQiPaoView">
        <attr name="color_of_arc" format="dimension|color"></attr>
        <attr name="animDuration" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="BubbleSelectValueView">
        <attr name="flag" format="integer"></attr>
        <attr name="start" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="MultipleSelectionButton">
        <attr name="ascv_unselectedTextColor" format="reference"/>
        <attr name="ascv_unselectedColor" format="reference"/>
        <attr name="ascv_selectedColor" format="reference"/>
        <attr name="ascv_selectedTextColor" format="reference"/>
        <attr name="ascv_items" format="reference"/>
        <attr name="ascv_values" format="reference"/>
        <attr name="ascv_equalWidth" format="boolean"/>
        <attr name="ascv_stretch" format="boolean"/>
        <attr name="ascv_defaultSelection" format="integer"/>
        <attr name="ascv_identifier" format="string"/>
    </declare-styleable>

    <declare-styleable name="SegmentControl">
        <attr name="cornerRadius" format="dimension|reference" />
        <attr name="colors" format="color|reference" />
        <attr name="texts" format="string|reference" />
        <attr name="android:textSize" />
        <attr name="direction" format="enum">
            <enum name="horizon" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="gaps" format="dimension|reference" />
        <attr name="horizonGap" format="dimension|reference" />
        <attr name="verticalGap" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="VernierView">
        <attr name="textsize" format="dimension"></attr>
        <attr name="levelsize" format="integer"></attr>
        <attr name="initiallevel" format="integer"></attr>
        <attr name="initialtext" format="string"></attr>
        <attr name="textunit" format="string"></attr>
        <attr name="decimal_type" format="enum">
            <enum name="DECIMAL_ZERO" value="0"></enum>
            <enum name="DECIMAL_ONE" value="1"></enum>
            <enum name="DECIMAL_TWO" value="2"></enum>
        </attr>
        <attr name="textcolor" format="reference"></attr>
    </declare-styleable>

</resources>