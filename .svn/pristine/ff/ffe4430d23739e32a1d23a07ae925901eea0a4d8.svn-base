<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_marginBottom="10dp"
    android:layout_height="match_parent">


    <RelativeLayout
        android:id="@+id/rl_calendar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
    </RelativeLayout>

    <include layout="@layout/common_history_timeline"/>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        >

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="20dp"
            >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                >
                <RelativeLayout
                    android:id="@+id/rl_gly_bloodsuger"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@drawable/bodyfat_low"
                    >

                    <TextView
                        android:id="@+id/tv_blood_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textColor="#ffffff"
                        android:textSize="17sp"
                        android:gravity="center"
                        />
                    <TextView
                        android:id="@+id/tv_blood_level"
                        android:layout_below="@id/tv_blood_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="偏低"
                        android:textColor="#ffffff"
                        android:textSize="16sp"
                        android:gravity="center"
                        />
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_bloodname"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="血糖"
                        android:textSize="17sp"
                        />
                    <TextView
                        android:id="@+id/tv_bloodstate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="(空腹)"
                        android:textSize="12sp"
                        />
                </LinearLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="(mmol/L)"
                    android:textSize="14sp"
                    android:gravity="center"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                >
                <RelativeLayout
                    android:id="@+id/rl_gly_chole"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@drawable/bodyfat_middle"
                    >

                    <TextView
                        android:id="@+id/tv_chole_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textColor="#ffffff"
                        android:textSize="17sp"
                        android:gravity="center"
                        />
                    <TextView
                        android:id="@+id/tv_chole_level"
                        android:layout_below="@id/tv_chole_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="正常"
                        android:textColor="#ffffff"
                        android:textSize="16sp"
                        android:gravity="center"
                        />

                </RelativeLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:text="总胆固醇"
                    android:textSize="17sp"
                    android:gravity="center"
                    android:layout_marginTop="5dp"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="(mmol/L)"
                    android:textSize="14sp"
                    android:gravity="center"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                >
                <RelativeLayout
                    android:id="@+id/rl_gly_tri"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@drawable/bodyfat_middle"
                    >

                    <TextView
                        android:id="@+id/tv_tri_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textColor="#ffffff"
                        android:textSize="17sp"
                        android:gravity="center"
                        />
                    <TextView
                        android:id="@+id/tv_tri_level"
                        android:layout_below="@id/tv_tri_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="正常"
                        android:textColor="#ffffff"
                        android:textSize="16sp"
                        android:gravity="center"
                        />

                </RelativeLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:text="甘油三脂"
                    android:textSize="17sp"
                    android:gravity="center"
                    android:layout_marginTop="5dp"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="(mmol/L)"
                    android:textSize="14sp"
                    android:gravity="center"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                >
                <RelativeLayout
                    android:id="@+id/rl_gly_highlip"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@drawable/circle_red"
                    >

                    <TextView
                        android:id="@+id/tv_hignlip_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textColor="#ffffff"
                        android:textSize="17sp"
                        android:gravity="center"
                        />
                    <TextView
                        android:id="@+id/tv_highlip_level"
                        android:layout_below="@id/tv_hignlip_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="偏高"
                        android:textColor="#ffffff"
                        android:textSize="16sp"
                        android:gravity="center"
                        />

                </RelativeLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:text="高密"
                    android:textSize="17sp"
                    android:gravity="center"
                    android:layout_marginTop="5dp"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="(mmol/L)"
                    android:textSize="14sp"
                    android:gravity="center"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                >
                <RelativeLayout
                    android:id="@+id/rl_gly_lowlip"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@drawable/bodyfat_middle"
                    >

                    <TextView
                        android:id="@+id/tv_lowlip_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textColor="#ffffff"
                        android:textSize="17sp"
                        android:gravity="center"
                        />
                    <TextView
                        android:id="@+id/tv_lowlip_level"
                        android:layout_below="@id/tv_lowlip_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="正常"
                        android:textColor="#ffffff"
                        android:textSize="16sp"
                        android:gravity="center"
                        />
                </RelativeLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:text="低密"
                    android:textSize="17sp"
                    android:gravity="center"
                    android:layout_marginTop="5dp"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="(mmol/L)"
                    android:textSize="14sp"
                    android:gravity="center"
                    />
            </LinearLayout>
        </TableRow>
    </TableLayout>
</LinearLayout>