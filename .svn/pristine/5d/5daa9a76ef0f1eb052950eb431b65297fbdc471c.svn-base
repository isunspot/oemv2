<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" >
        <shape>
            <stroke android:width="1dp" android:color="#28BFC9"/>
            <corners android:bottomLeftRadius="0dp" android:bottomRightRadius="5dp"
                android:topLeftRadius="0dp" android:topRightRadius="5dp"/>
            <solid android:color="#28BFC9"/>
        </shape>
    </item>
    <item android:state_checked="false" >
        <shape>
            <stroke android:width="1dp" android:color="#28BFC9"/>
            <corners android:bottomLeftRadius="0dp" android:bottomRightRadius="5dp"
                android:topLeftRadius="0dp" android:topRightRadius="5dp"/>
            <solid android:color="#D3F1E7"/>
        </shape>
    </item>
</selector>