package com.capitalbio.oemv2.myapplication.Bean;

import android.widget.RadioGroup;

import com.capitalbio.oemv2.myapplication.R;

/**
 * Created by wxy on 16-2-24.
 */
public class DiabeteFactor {
    private String waistSize;

    private String wayOfLife;

    private String sugarRegulationDamaged;

    private String acceptTreatmentOfBloodLipid;

    private String acceptTreatmentOfPressure;

    private String sclerosisOfArterialCongeeCardiovascularDiseas;

    private String historyOfSexSteroidDiabetes;

    private String pcos;

    private String giantBaby;

    private String gdm;

    private String longtermAntipsychoticDrugs;

    private String parentType2diabetes;
    private RadioGroup[] radioGroups = new RadioGroup[12];

    public String getWaistSize() {
        return waistSize;
    }

    public void setWaistSize(String waistSize) {
        this.waistSize = waistSize;
    }

    public void setWayOfLife(String wayOfLife){
        this.wayOfLife = wayOfLife;
    }
    public String getWayOfLife(){
        return this.wayOfLife;
    }
    public void setSugarRegulationDamaged(String sugarRegulationDamaged){
        this.sugarRegulationDamaged = sugarRegulationDamaged;
    }
    public String getSugarRegulationDamaged(){
        return this.sugarRegulationDamaged;
    }
    public void setAcceptTreatmentOfBloodLipid(String acceptTreatmentOfBloodLipid){
        this.acceptTreatmentOfBloodLipid = acceptTreatmentOfBloodLipid;
    }
    public String getAcceptTreatmentOfBloodLipid(){
        return this.acceptTreatmentOfBloodLipid;
    }
    public void setAcceptTreatmentOfPressure(String acceptTreatmentOfPressure){
        this.acceptTreatmentOfPressure = acceptTreatmentOfPressure;
    }
    public String getAcceptTreatmentOfPressure(){
        return this.acceptTreatmentOfPressure;
    }
    public void setSclerosisOfArterialCongeeCardiovascularDiseas(String sclerosisOfArterialCongeeCardiovascularDiseas){
        this.sclerosisOfArterialCongeeCardiovascularDiseas = sclerosisOfArterialCongeeCardiovascularDiseas;
    }
    public String getSclerosisOfArterialCongeeCardiovascularDiseas(){
        return this.sclerosisOfArterialCongeeCardiovascularDiseas;
    }
    public void setHistoryOfSexSteroidDiabetes(String historyOfSexSteroidDiabetes){
        this.historyOfSexSteroidDiabetes = historyOfSexSteroidDiabetes;
    }
    public String getHistoryOfSexSteroidDiabetes(){
        return this.historyOfSexSteroidDiabetes;
    }
    public void setPcos(String pcos){
        this.pcos = pcos;
    }
    public String getPcos(){
        return this.pcos;
    }
    public void setGiantBaby(String giantBaby){
        this.giantBaby = giantBaby;
    }
    public String getGiantBaby(){
        return this.giantBaby;
    }
    public void setGdm(String gdm){
        this.gdm = gdm;
    }
    public String getGdm(){
        return this.gdm;
    }
    public void setLongtermAntipsychoticDrugs(String longtermAntipsychoticDrugs){
        this.longtermAntipsychoticDrugs = longtermAntipsychoticDrugs;
    }
    public String getLongtermAntipsychoticDrugs(){
        return this.longtermAntipsychoticDrugs;
    }
    public void setParentType2diabetes(String parentType2diabetes){
        this.parentType2diabetes = parentType2diabetes;
    }
    public String getParentType2diabetes(){
        return this.parentType2diabetes;
    }


    public void setRadioGroups(RadioGroup[] radiogroups){

        this.radioGroups =radiogroups;
        if("静坐".equals(wayOfLife)){
            radioGroups[0].check(R.id.rg_diabete_2_yes);
        }
        if("运动".equals(wayOfLife)){
            radioGroups[0].check(R.id.rg_diabete_2_no);
        }
        if("是".equals(sugarRegulationDamaged)){
            radioGroups[1].check(R.id.rg_diabete_3_yes);
        }
        if("否".equals(sugarRegulationDamaged)){
            radioGroups[1].check(R.id.rg_diabete_3_no);
        }
        if("是".equals(acceptTreatmentOfBloodLipid)){
            radioGroups[2].check(R.id.rg_diabete_4_yes);
        }
        if("否".equals(acceptTreatmentOfBloodLipid)){
            radioGroups[2].check(R.id.rg_diabete_4_no);
        }
        if("是".equals(acceptTreatmentOfPressure)){
            radioGroups[3].check(R.id.rg_diabete_5_yes);
        }
        if("否".equals(acceptTreatmentOfPressure)){
            radioGroups[3].check(R.id.rg_diabete_5_no);
        }
        if("是".equals(sclerosisOfArterialCongeeCardiovascularDiseas)){
            radioGroups[4].check(R.id.rg_diabete_6_yes);
        }
        if("否".equals(sclerosisOfArterialCongeeCardiovascularDiseas)){
            radioGroups[4].check(R.id.rg_diabete_6_no);
        }
        if("是".equals(historyOfSexSteroidDiabetes)){
            radioGroups[5].check(R.id.rg_diabete_7_yes);
        }
        if("否".equals(historyOfSexSteroidDiabetes)){
            radioGroups[5].check(R.id.rg_diabete_7_no);
        }
        if("是".equals(pcos)){
            radioGroups[6].check(R.id.rg_diabete_8_yes);
        }
        if("否".equals(pcos)){
            radioGroups[6].check(R.id.rg_diabete_8_no);
        }
        if("是".equals(giantBaby)){
            radioGroups[7].check(R.id.rg_diabete_9_yes);
        }
        if("否".equals(giantBaby)){
            radioGroups[7].check(R.id.rg_diabete_9_no);
        }
        if("是".equals(gdm)){
            radioGroups[8].check(R.id.rg_diabete_10_yes);
        }
        if("否".equals(gdm)){
            radioGroups[8].check(R.id.rg_diabete_10_no);
        }
        if("是".equals(longtermAntipsychoticDrugs)){
            radioGroups[9].check(R.id.rg_diabete_11_yes);
        }
        if("否".equals(longtermAntipsychoticDrugs)){
            radioGroups[9].check(R.id.rg_diabete_11_no);
        }
        if("是".equals(parentType2diabetes)){
            radioGroups[10].check(R.id.rg_diabete_12_yes);
        }
        if("否".equals(parentType2diabetes)){
            radioGroups[10].check(R.id.rg_diabete_12_no);
        }
    }
}
