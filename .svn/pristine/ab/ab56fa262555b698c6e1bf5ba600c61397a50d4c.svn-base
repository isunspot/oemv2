package com.capitalbio.oemv2.myapplication.Bean;

import org.xutils.db.annotation.Column;
import org.xutils.db.annotation.Table;

/**
 * Created by chengkun on 15-12-19.
 */
@Table(name = "SportDataDetailBean")
public class SportDataDetailBean {

    //记录id
    @Column(name = "id", isId = true)

    private int id;

    //设备id mac
    @Column(name = "deviceId")
    private String deviceId;

    @Column(name = "username")
    private String username;

    //运动类型
    @Column(name = "sportType")
    private String sportType;

    //采样点时间
    @Column(name = "samplePointTime")
    private String samplePointTime;

    //采样点步数
    @Column(name = "steps")
    private int steps;

    //采样点距离
    @Column(name = "distance")
    private int distance;

    //卡路里
    @Column(name = "cal")
    private int cal;

    //数据索引
    @Column(name = "index")
    private int index;

    //是否上传
    @Column(name = "isUpload")
    private boolean isUpload;

    //数据来源
    @Column(name = "dataSource")
    private String dataSource;

    @Column(name = "longTime")
    private long longTime;

    @Column(name = "ymd")
    private String ymd;


    @Column(name = "hour")
    private String hour;

    @Column(name = "day")
    private String day;
    //数据记录创建时间
    @Column(name = "ctime")
    private String ctime;

    public String getDay() {
        return day;
    }

    public void setDay(String day) {
        this.day = day;
    }

    public void setDeviceId(String  deviceId) {
        this.deviceId = deviceId;
    }

    public void setSportType(String sportType) {
        this.sportType = sportType;
    }

    public void setSamplePointTime(String samplePointTime) {
        this.samplePointTime = samplePointTime;
    }

    public void setSteps(int steps) {
        this.steps = steps;
    }

    public void setCal(int cal) {
        this.cal = cal;
    }

    public void setIndex(int index) {
        this.index = index;
    }

    public void setIsUpload(boolean isUpload) {
        this.isUpload = isUpload;
    }

    public void setDataSource(String dataSource) {
        this.dataSource = dataSource;
    }

    public void setDistance(int distance) {
        this.distance = distance;
    }


    public String getDeviceId() {
        return deviceId;
    }

    public String getSportType() {
        return sportType;
    }

    public String getSamplePointTime() {
        return samplePointTime;
    }

    public int getSteps() {
        return steps;
    }

    public int getDistance() {
        return distance;
    }

    public int getCal() {
        return cal;
    }

    public int getIndex() {
        return index;
    }

    public boolean isUpload() {
        return isUpload;
    }

    public String getDataSource() {
        return dataSource;
    }

    public long getLongTime() {
        return longTime;
    }

    public void setLongTime(long longTime) {
        this.longTime = longTime;
    }

    public String getYmd() {
        return ymd;
    }

    public void setYmd(String ymd) {
        this.ymd = ymd;
    }

    public String getHour() {
        return hour;
    }

    public void setHour(String hour) {
        this.hour = hour;
    }

    public String getUsername() {
        return username;
    }

    public void setUsername(String username) {
        this.username = username;
    }

    public String getCtime() {
        return ctime;
    }

    public void setCtime(String ctime) {
        this.ctime = ctime;
    }
}
